    "doctor-zhivago": "1.4.6",
    "express": "4.17.1",
    "express-validation": "^3.0.8",
    "http-status": "^1.5.0",
    "lodash": "4.17.21",
    "moment": "^2.29.1",
    "passport": "^0.5.0",
    "passport-http": "^0.3.0",
    "password-validator": "^5.1.1",
    "redis": "^3.1.2",
